{
  "expo": {
    "name": "mobileApplication",
    "slug": "mobileApplication",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img_3.jpg",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow ${mobileApplication} to access your camera"
        }
      ]
    ],
    "splash": {
      "image": "./assets/img_3.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img_3.jpg",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/img_3.jpg"
    }
  }
}
